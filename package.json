{
  "name": "musikedemo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "sync:ios": "yarn rimraf ../ios && yarn cpx 'node_modules/react-native-track-player/ios/**' ../ios",
    "sync:android": "yarn rimraf ../android && yarn cpx 'node_modules/react-native-track-player/android/**' ../android",
    "sync": "yarn sync:ios && yarn sync:android",
    "generate": "npx react-native generate-bootsplash ./src/assets/original_icon.png --background-color=1a1a1a   --logo-width=180 --assets-path=./src/assets --flavor=main"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.6",
    "@react-native-community/slider": "^4.2.2",
    "@react-navigation/drawer": "^6.4.1",
    "@react-navigation/native": "^6.0.10",
    "@rimiti/react-native-toastify": "^1.3.0",
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-native-bootsplash": "^4.1.6",
    "react-native-fast-image": "^8.5.11",
    "react-native-gesture-handler": "^2.4.2",
    "react-native-modal": "^13.0.1",
    "react-native-reanimated": "^2.8.0",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-snap-carousel": "^3.9.1",
